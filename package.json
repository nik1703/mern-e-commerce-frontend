    "js-cookie": "^3.0.5",
    "moment": "^2.30.1",
    "mongoose": "^8.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^5.0.1",
